		"lion-system": "^1.0.11",
		"lionp": "^1.0.19",
		"prettier": "^2.5.1",
		"tsc-alias": "^1.6.4",
		"typescript": "^4.6.2",
		"vitest": "^0.5.9"
	},
	"prettier": "@leonzalion/configs/prettier.cjs",
	"commitlint": {
